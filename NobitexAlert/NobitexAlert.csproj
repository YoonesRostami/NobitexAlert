<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Alerts\alert1.wav" />
    <None Remove="Alerts\alert2.wav" />
    <None Remove="Alerts\alert3.wav" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Alerts\alert1.wav" />
    <EmbeddedResource Include="Alerts\alert2.wav" />
    <EmbeddedResource Include="Alerts\alert3.wav" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NoobsMuc.Coinmarketcap.Client" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>